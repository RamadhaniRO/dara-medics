{
  "version": 2,
  "name": "dara-medics-backend",
  "builds": [
    {
      "src": "src/serverless-simple.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/serverless-simple.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "3000"
  }
}
